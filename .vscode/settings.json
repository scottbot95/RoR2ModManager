{
  "files.exclude": {
    "dist": true,
    "tmp": true,
    "out-tsc": true,
    "app-builds": true,
    "release": true,
    "**/main.js": true,
    "electron/**/*.js": true,
    "src/**/*.js": true,
    "src/karma.conf.js": false,
    "**/*.js.map": true,
    "node_modules": true,
    ".idea": true,
    "**/.project": true,
    "**/.classpath": true,
    "**/.c9/": true,
    "**/*.launch": true,
    "**/.settings/": true,
    "**/*.sublime-workspace": true,
    ".vscode/*": false,
    ".vscode/settings.json": false,
    ".vscode/tasks.json": false,
    ".vscode/launch.json": false,
    ".vscode/extensions.json": false,
    ".sass-cache": true,
    "connect.lock": true,
    "coverage": true,
    "libpeerconnection.log": true,
    "**/npm-debug.log": true,
    "**/testem.log": true,
    "typings": true,
    "e2e/*.js": true,
    "e2e/protractor.conf.js": false,
    "e2e/*.map": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "npm.packageManager": "yarn"
}
